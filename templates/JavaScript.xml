<templateSet group="JavaScript">
  <template name="log" value="console.log($END$);" description="console.log" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="it" value="it('$1$', () =&gt; {&#10;    $END$&#10;});" description="it should..." toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="d" value="describe('$1$', () =&gt; {&#10;    $END$&#10;});" description="describe" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="b" value="beforeEach(() =&gt; {&#10;    $END$&#10;});" description="beforeEach" toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="f" value="($1$) =&gt; {&#10;    $END$        &#10;}" description="function" toReformat="true" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="e" value="expect($CURSOR$)$END$;" description="expect" toReformat="false" toShortenFQNames="true">
    <variable name="CURSOR" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="nf" value="function $name$ ($args$) {&#10;    $end$&#10;}" description="Named function" toReformat="true" toShortenFQNames="true">
    <variable name="name" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="args" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="end" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="pp" value="console.log(JSON.stringify($1$, null, 4));" description="Pretty print" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="ep" value="expect($1$).to.have.property($2$).that.is.equal($3$);" description="expect property" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="2" expression="" defaultValue="" alwaysStopAt="true" />
    <variable name="3" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="cl" value="private static readonly logger = rootLogger$import$.create($CLASS$);&#10;" description="create logger" toReformat="false" toShortenFQNames="true">
    <variable name="import" expression="completeSmart()" defaultValue="" alwaysStopAt="true" />
    <variable name="CLASS" expression="jsClassName()" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="spec" value="import {expect} from 'chai';&#10;&#10;describe('$ClassName$', () =&gt; {&#10;    $END$&#10;});&#10;" description="" toReformat="false" toShortenFQNames="true">
    <variable name="ClassName" expression="capitalize(camelCase(substringBefore(fileName(), &quot;.&quot;))) " defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="TypeScript" value="true" />
    </context>
  </template>
  <template name="af" value="async ($1$) =&gt; {&#10;    $END$        &#10;}" description="async function" toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="ait" value="it('$1$', async () =&gt; {&#10;    $END$&#10;});" description="async it should..." toReformat="false" toShortenFQNames="true">
    <variable name="1" expression="" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="p" value="Promise&lt;$type$&gt;" description="Promise" toReformat="true" toShortenFQNames="true">
    <variable name="type" expression="&quot;void&quot;" defaultValue="" alwaysStopAt="true" />
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
  <template name="of" value="instanceof " toReformat="false" toShortenFQNames="true">
    <context>
      <option name="JAVA_SCRIPT" value="true" />
    </context>
  </template>
</templateSet>